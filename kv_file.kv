<ProductsPage>
    name:'ProductsPage'
    MDBoxLayout:
        orientation: 'vertical'
        MDToolbar:
            title:app.custom_name
            left_action_items: [["menu", lambda x: app.toggle_nav()]]
        # MDBoxLayout:
        #     orientation: 'vertical'
            # md_bg_color: .8,.8,.1,1
        ScrollView:
            # do_scroll_x: False
            size: root.size
            MDGridLayout:
                cols:2
                id:ProductsPage
                size_hint_y: None
                height: self.minimum_height  # required to size GridLayout
                padding: 10
                spacing: 10
                
                







<AboutPage>
    name:'AboutPage'
    MDBoxLayout:
        orientation: 'vertical'
        MDToolbar:
            title:'name'
            left_action_items: [["menu", lambda x: app.toggle_nav()]]
        MDBoxLayout:
            padding:5
            MDCard:
                radius:[15,]
                elevation:20
                padding:10
                MDGridLayout:
                    cols:1
                    MDCard:
                        elevation:20
                        size_hint_y:.45
                        padding:10
                        Image:
                            source:'src/company_logo.jpg'
                    MDCard:
                        elevation:20
                        padding:20
                        MDGridLayout:
                            cols:1
                            MDLabel:
                                text:'Company Name'
                                font_name: 'font/Chopsic-K6Dp.ttf'
                                font_size:dp(30)
                                adaptive_height:True
                            MDLabel:
                                adaptive_height:True
                                text:'\n Company info Goes Here\n\n'
                                font_size:dp(15)
                            MDLabel:
                                text:'ADRESS'
                                font_name: 'font/Chopsic-K6Dp.ttf'
                                font_size:dp(20)
                                adaptive_height:True
                            MDLabel:
                                adaptive_height:True
                                text:'\n Address Goes Here \n\n'
                                font_size:dp(15)
                            MDLabel:
                                text:'Phone / website'
                                font_name: 'font/Chopsic-K6Dp.ttf'
                                font_size:dp(20)
                                adaptive_height:True
                            MDLabel:
                                adaptive_height:True
                                text:'\n +92 123 4564 \n abc@yourcompany.com \n\n'
                                font_size:dp(15)
    

    
<ProductsDeatailsPage>
    name:'ProductsDeatailsPage'
    
    MDBoxLayout:
        orientation: 'vertical'
        md_bg_color:app.theme_cls.primary_dark
        MDCard:
            elevation:10
            size_hint_y:root.height/3
            md_bg_color: app.theme_cls.primary_dark
            padding:dp(20)
            Image:
                id: productInfoImage
                source:'src/shirts/shirt1.jpg'
        MDCard:
            elevation:10
            size_hint_y: root.height/2
            radius:[25,25,0,0]
            padding: ('20dp', '0dp', '20dp', '20dp')
            
            MDBoxLayout:
                orientation: 'vertical'
                MDBoxLayout:
                    
                    orientation: 'vertical'
                    size_hint_y:.2
                    MDIconButton:
                        adaptive_size:True
                        icon:'tooltip-remove-outline'	
                        pos_hint:{'right':1}
                        on_release:app.onCross()

                    MDSeparator:	
                MDGridLayout:
                # MDBoxLayout:
                    cols:1
                    
                    # orientation: 'vertical'
                    
                    MDLabel:
                        text:'Title'
                        font_style:'Button'
                        font_size:dp(25)
                        size_hint_y:None
                        height:dp(50)
                        # adaptive_height:True
                        
                    
                    MDLabel:
                        text:'Soccer short sleeve Shirt'
                        font_style:'Caption'
                        font_size:dp(15)
                        # size_hint_y:None
                        # height:dp(5)
                        adaptive_height:True
                    MDBoxLayout:
                        adaptive_height:True
                        MDLabel:
                            text:'Price'
                            font_style:'Button'
                            font_size:dp(20)
                            size_hint_x:.3	
                            size_hint_y:1.2				
                            # adaptive_height:True
                        MDLabel:
                            text:'$50'
                            font_style:'Caption'
                            font_size:dp(15)
                            size_hint_y:1.2				
                            
                            
                
                    
                    
                
                
                    