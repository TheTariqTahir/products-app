MDGridLayout:
    cols:1
    MDToolbar:
        title:app.custom_name           
        left_action_items: [["menu", lambda x: app.toggle_nav()]]
        right_action_items: [["theme-light-dark", lambda x: app.change_theme()]]



    MDNavigationLayout:
        ScreenManager:
            id : screen_manager
            MainPage:
            ProductsDeatailsPage:
            ProductsPage:
            WelcomeScreen:
            AboutPage:
            DevaloperPage:
        

        MDNavigationDrawer:
            id: nav_drawer
            size_hint: .75,None
            height: root.height
            
            MDGridLayout:
                cols:1
                id:boxes
                FitImage:
                    source:'src/company_logo.jpg'
                    size_hint:1,None
                    height:dp(200)
                    # width:dp(700)
                    # keep_ratio: True
                    allow_stretch:True
                MDGridLayout:
                    cols:1
                    spacing:4
                    padding:15
                    col_default_width:self.width-30
                    MDCard:
                        radius:[10]
                        id:nav_item
                        # md_bg_color:app.theme_cls.primary_color
                        
                        elevation:20

                        size_hint:1,None
                        
                        
                        height:root.height/12
                        pos_hint:{'center_x':.5}
                        orientation: 'horizontal'
                        on_release: 
                            app.change_screen('MainPage','left')
                            app.toggle_nav()
                            app.change_nav_drawer_color(self)
                        
                        
                        MDIconButton:
                            icon:'home'
                            pos_hint:{'center_y':.5}
                        MDLabel:
                            theme_text_color : 'Custom'
                            text_color : app.theme_cls.primary_color
                            text:'Main Page'
                    
                    MDCard:
                        radius:[10]
                        id:nav_item
                        # md_bg_color:(1,1,.3,1)
                        elevation:20

                        size_hint:1,None
                        height:root.height/12
                        pos_hint:{'center_x':.5}
                        orientation: 'horizontal'
                        # on_press:print('clicked')
                        padding: ('10dp', '0dp', '0dp', '0dp')
                        spacing:10
                        on_release: 
                            app.change_screen('AboutPage','left')
                            app.toggle_nav()
                            app.change_nav_drawer_color(self)
                        FitImage:
                            # icon:'face-profile'
                            source:'src/company_logo.jpg'
                            size_hint:None,None
                            width:dp(30)
                            height:dp(30)
                            pos_hint:{'center_y':.5}
                        MDLabel:
                            text:'Company Info'

                  
                MDBoxLayout:
                    orientation: 'vertical'
                    padding:15
                    
                    MDCard:
                        radius:[10]
                        id:nav_item
                        # md_bg_color:(1,1,.3,1)
                        elevation:20

                        size_hint:1,None
                        height:root.height/12
                        pos_hint:{'center_x':.5}
                        orientation: 'horizontal'
                        on_release: 
                            app.change_screen('DevaloperPage','left')
                            app.toggle_nav()
                            app.change_nav_drawer_color(self)
                        MDIconButton:
                            icon:'account-cowboy-hat'
                            pos_hint:{'center_y':.5}
                        MDLabel:
                            text:'About Developer'
            

			

