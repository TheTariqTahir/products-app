
MDNavigationLayout:
    ScreenManager:
        id : screen_manager
        MainPage:
        ProductsDeatailsPage:
        ProductsPage:
        WelcomeScreen:
        AboutPage:
       

    MDNavigationDrawer:
        id: nav_drawer
        size_hint: .75,1
        
        MDGridLayout:
            cols:1
            id:boxes
            FitImage:
                source:'src/company_logo.jpg'
                size_hint:1,None
                height:dp(200)
                # width:dp(700)
                # keep_ratio: True
                allow_stretch:True
            MDGridLayout:
                cols:1
                spacing:4
                padding:15
                
                MDCard:
                    md_bg_color:app.theme_cls.primary_color
                    elevation:12

                    size_hint:1,None
                    height:root.height/11
                    pos_hint:{'center_x':.5}
                    orientation: 'horizontal'
                    on_release: 
                        app.change_screen('MainPage','left')
                        app.toggle_nav()
                        app.change_nav_drawer_color(self)
                    
                    MDIconButton:
                        icon:'home'
                        pos_hint:{'center_y':.5}
                    MDLabel:
                        text:'Main Page'
                
                MDCard:
                    # md_bg_color:(1,1,.3,1)
                    elevation:12

                    size_hint:1,None
                    height:root.height/11
                    pos_hint:{'center_x':.5}
                    orientation: 'horizontal'
                    on_press:print('clicked')
                    padding: ('10dp', '0dp', '0dp', '0dp')
                    spacing:10
                    on_release: 
                        app.change_screen('AboutPage','left')
                        app.toggle_nav()
                        app.change_nav_drawer_color(self)
                    FitImage:
                        # icon:'face-profile'
                        source:'src/company_logo.jpg'
                        size_hint:None,None
                        width:dp(30)
                        height:dp(30)
                        
                        
                        
                        pos_hint:{'center_y':.5}
                    MDLabel:
                        text:'Company Info'
            MDBoxLayout:
                orientation: 'vertical'
                padding:15
                
                MDCard:
                    # md_bg_color:(1,1,.3,1)
                    elevation:12

                    size_hint:1,None
                    height:root.height/11
                    pos_hint:{'center_x':.5}
                    orientation: 'horizontal'
                    on_press:print('clicked')
                    MDIconButton:
                        icon:'account-cowboy-hat'
                        pos_hint:{'center_y':.5}
                    MDLabel:
                        text:'About Developer'
		

			

<WelcomeScreen>
    name:'WelcomeScreen'

<MainPage>
    name:'MainPage'
    
    MDBoxLayout:
        orientation: 'vertical'
        padding: ('0dp', '0dp', '0dp', '25dp')
        spacing:15
        MDToolbar:
            title:app.custom_name
            left_action_items: [["menu", lambda x: app.toggle_nav()]]
            # pos_hint:{'top':1}
            # size_hint: 1,.1
        MDCard:
            # md_bg_color: .8,.8,.1,1
            radius:[25,]
            size_hint: .95,.4
            pos_hint: {'center_x':.5}
            elevation:24
            # md_bg_color:app.theme_cls.primary_dark
            MDLabel:
                text:'New and Stylish'
                font_name: 'font/Sweet Valentine.ttf'
                halign:'center'
                font_size:dp(36)
        MDCard:
            # md_bg_color: .8,.8,.1,1
            radius:[25,]
            size_hint: .95,.87
            pos_hint: {'center_x':.5}
            elevation:15
            MDBoxLayout:
                orientation: 'vertical'
                padding: ('0dp', '0dp', '0dp', '5dp')
                # size_hint_x:None
                width:self.width
                MDBoxLayout:                    
                    size_hint:1,.35
                    padding: ('25dp', '0dp', '0dp', '0dp')
